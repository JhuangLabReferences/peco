eset_final <- readRDS("/project2/gilad/joycehsiao/fucci-seq/data/eset-final.rds")

genes_cyclic_list <- get(load("data/genes_cyclic_list.rda"))

eset_final_sub <- eset_final[match(genes_cyclic_list$ensg, rownames(eset_final)),]

all.equal(rownames(eset_final_sub), genes_cyclic_list$ensg)

eset_final_sub <- eset_final_sub[1:100,]

save(eset_final_sub, file="data/eset_final_sub.rda")

