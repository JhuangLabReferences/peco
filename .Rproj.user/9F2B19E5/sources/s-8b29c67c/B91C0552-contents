fits_all <- readRDS("/project2/gilad/joycehsiao/fucci-seq/output/npreg-trendfilter-quantile.Rmd/fit.quant.rds")

pve <- sapply(fits_all, "[[", 3)
cyclic_gene <- data.frame(order=order(pve, decreasing = T),
                          ensg=names(fits_all),
                          pve=sapply(fits_all, "[[", 3),
                          stringsAsFactors = F)
cyclic_gene <- cyclic_gene[cyclic_gene$order,]
genes_cyclic_list <- cyclic_gene

save(genes_cyclic_list, file="data/genes_cyclic_list.Rdata")
